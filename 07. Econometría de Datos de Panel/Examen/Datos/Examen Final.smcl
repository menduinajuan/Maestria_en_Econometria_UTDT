. *PROGRAMA*
. 
. program define simulation2, rclass
{txt}  1{com}. 
.         *Seteo*
.         drop _all
{txt}  2{com}.         local N=$N
{txt}  3{com}.         local T=${c -(}T{c )-}+10
{txt}  4{com}.         local NT=`N'*`T'
{txt}  5{com}.         set obs `NT'
{txt}  6{com}.         egen id=seq(), from(1) to(`N') block(`T')
{txt}  7{com}.         egen t=seq(), from(1) to(`T')
{txt}  8{com}.         xtset id t
{txt}  9{com}. 
.         *Variables*
.         generate c=.
{txt} 10{com}.         forvalues id=1(1)`N' {c -(}
{txt} 11{com}.                 local aux=rnormal()
{txt} 12{com}.                 replace c=`aux' if (id==`id')
{txt} 13{com}.         {c )-}
{txt} 14{com}.         generate u=rnormal()
{txt} 15{com}.         generate v=rnormal(0,0.9^(1/2))
{txt} 16{com}.         generate x=0
{txt} 17{com}.         replace x=0.8*x[_n-1]+v if t>=2
{txt} 18{com}.         generate y=0
{txt} 19{com}.         replace y=${c -(}alpha{c )-}*y[_n-1]+${c -(}beta{c )-}*x+c+u if t>=2
{txt} 20{com}.         generate yL1=L.y
{txt} 21{com}.         generate yL2=L.yL1
{txt} 22{com}.         generate yD1=y-y[_n-1]
{txt} 23{com}.         generate yL1D1=yL1-yL1[_n-1]
{txt} 24{com}.         generate xD1=x-x[_n-1]
{txt} 25{com}.         keep if (t>10)
{txt} 26{com}.         replace t=t-10
{txt} 27{com}.         sort id t
{txt} 28{com}. 
.         *LSDV*
.         regress y yL1 x i.id, robust
{txt} 29{com}.         return scalar alpha_lsdv=e(b)[1,1]
{txt} 30{com}.         test yL1=${c -(}alpha{c )-}
{txt} 31{com}.         return scalar pvalue_alpha_lsdv=r(p)
{txt} 32{com}.         if (${c -(}ej{c )-}==6) {c -(}
{txt} 33{com}.                 return scalar beta_lsdv=e(b)[1,2]
{txt} 34{com}.                 test x=${c -(}beta{c )-}
{txt} 35{com}.                 return scalar pvalue_beta_lsdv=r(p)
{txt} 36{com}.         {c )-}
{txt} 37{com}. 
.         *AB GMM1*
.         xtabond2 y yL1 x, gmmstyle(L.y) ivstyle(x) robust noleveleq
{txt} 38{com}.         return scalar alpha_ab_gmm1=e(b)[1,1]
{txt} 39{com}.         test yL1=${c -(}alpha{c )-}
{txt} 40{com}.         return scalar pvalue_alpha_ab_gmm1=r(p)
{txt} 41{com}.         if (${c -(}ej{c )-}==6) {c -(}
{txt} 42{com}.                 return scalar beta_ab_gmm1=e(b)[1,2]
{txt} 43{com}.                 test x=${c -(}beta{c )-}
{txt} 44{com}.                 return scalar pvalue_beta_ab_gmm1=r(p)
{txt} 45{com}.         {c )-}
{txt} 46{com}. 
.         *AB GMM2*
.         xtabond2 y yL1 x, gmmstyle(L.y) ivstyle(x) twostep robust noleveleq
{txt} 47{com}.         return scalar alpha_ab_gmm2=e(b)[1,1]
{txt} 48{com}.         test yL1=${c -(}alpha{c )-}
{txt} 49{com}.         return scalar pvalue_alpha_ab_gmm2=r(p)
{txt} 50{com}.         if (${c -(}ej{c )-}==6) {c -(}
{txt} 51{com}.                 return scalar beta_ab_gmm2=e(b)[1,2]
{txt} 52{com}.                 test x=${c -(}beta{c )-}
{txt} 53{com}.                 return scalar pvalue_beta_ab_gmm2=r(p)
{txt} 54{com}.         {c )-}
{txt} 55{com}. 
.         *BB GMM1*
.         xtabond2 y yL1 x, gmmstyle(L.y) ivstyle(x, equation(level)) robust
{txt} 56{com}.         return scalar alpha_bb_gmm1=e(b)[1,1]
{txt} 57{com}.         test yL1=${c -(}alpha{c )-}
{txt} 58{com}.         return scalar pvalue_alpha_bb_gmm1=r(p)
{txt} 59{com}.         if (${c -(}ej{c )-}==6) {c -(}
{txt} 60{com}.                 return scalar beta_bb_gmm1=e(b)[1,2]
{txt} 61{com}.                 test x=${c -(}beta{c )-}
{txt} 62{com}.                 return scalar pvalue_beta_bb_gmm1=r(p)
{txt} 63{com}.         {c )-}
{txt} 64{com}. 
.         *AH*
.         ivregress 2sls yD1 (yL1D1=yL2) xD1, noconstant robust
{txt} 65{com}.         return scalar alpha_ah=e(b)[1,1]
{txt} 66{com}.         test yL1D1=${c -(}alpha{c )-}
{txt} 67{com}.         return scalar pvalue_alpha_ah=r(p)
{txt} 68{com}.         if (${c -(}ej{c )-}==6) {c -(}
{txt} 69{com}.                 return scalar beta_ah=e(b)[1,2]
{txt} 70{com}.                 test xD1=${c -(}beta{c )-}
{txt} 71{com}.                 return scalar pvalue_beta_ah=r(p)
{txt} 72{com}.         {c )-}
{txt} 73{com}. 
.         *KIVIET*
.         xtlsdvc y x, initial(ah) bias(2)
{txt} 74{com}.         return scalar alpha_kiviet=e(b)[1,1]
{txt} 75{com}.         scalar alpha_lsdvc=e(b)[1,1]
{txt} 76{com}.         scalar beta_lsdvc=e(b)[1,2]
{txt} 77{com}.         scalar NT=e(N)
{txt} 78{com}.         scalar T=e(Tbar)
{txt} 79{com}.         scalar N=e(N_g)
{txt} 80{com}.         scalar K=colsof(e(b))
{txt} 81{com}.         egen mean_y=mean(y), by(id)
{txt} 82{com}.         egen mean_yL1=mean(yL1), by(id)
{txt} 83{com}.         egen mean_x=mean(x), by(id)
{txt} 84{com}.         generate within_y=y-mean_y
{txt} 85{com}.         generate within_yL1=yL1-mean_yL1
{txt} 86{com}.         generate within_x=yL1-mean_x
{txt} 87{com}.         generate e=within_y-alpha_lsdvc*within_yL1-beta_lsdvc*within_x
{txt} 88{com}.         matrix accum ZtZ=within_yL1 within_x, noconstant
{txt} 89{com}.         matrix accum ete=e, noconstant
{txt} 90{com}.         matrix define s2=ete/(NT-N-T-K+1)
{txt} 91{com}.         matrix define V=s2*invsym(ZtZ)
{txt} 92{com}.         scalar se_alpha_lsdvc=sqrt(V[1,1])
{txt} 93{com}.         scalar tstat_alpha=(alpha_lsdvc-${c -(}alpha{c )-})/se_alpha_lsdvc
{txt} 94{com}.         scalar pvalue_alpha=2*ttail(NT-K,abs(tstat_alpha))
{txt} 95{com}.         return scalar pvalue_alpha_kiviet=pvalue_alpha
{txt} 96{com}.         if (${c -(}ej{c )-}==6) {c -(}
{txt} 97{com}.                 return scalar beta_kiviet=e(b)[1,2]
{txt} 98{com}.                 scalar se_beta_lsdvc=sqrt(V[2,2])
{txt} 99{com}.                 scalar tstat_beta=(beta_lsdvc-${c -(}beta{c )-})/se_beta_lsdvc
{txt}100{com}.                 scalar pvalue_beta=2*ttail(NT-K,abs(tstat_beta))
{txt}101{com}.                 return scalar pvalue_beta_kiviet=pvalue_beta
{txt}102{com}.         {c )-}
{txt}103{com}. 
. end
{txt}
{com}. 
